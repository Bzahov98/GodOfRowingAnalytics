<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bzahov.godofrowing.analytics">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".general.RowAnalyticsApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_round_rowing_24"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GodOfRowingAnalytics">
        <activity
            android:name=".general.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.GodOfRowingAnalytics">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity><activity
            android:name=".screens.testStuff.TestOnBoard"
            android:exported="true"
            android:theme="@style/Theme.GodOfRowingAnalytics">

        </activity>
    </application>

</manifest>